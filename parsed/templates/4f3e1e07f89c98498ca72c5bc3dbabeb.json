{"components/f/hero/carousel":"\n    <section class=\"heroCarousel\"\n        data-slick='{\n            \"arrows\": {{arrows}},\n            \"mobileFirst\": true,\n            \"slidesToShow\": 1,\n            \"slidesToScroll\": 1,\n            \"autoplay\": true,\n            \"autoplaySpeed\": {{carousel.swap_frequency}},\n            \"lazyLoad\": \"anticipated\"\n        }'>\n        \n       <a href=\"/custom-categ/\">\n            <div class=\"heroCarousel-slide\">\n                <div class=\"heroCarousel-image-wrapper\">\n                    <img style=\"height: auto; width: 100% !important;\" src=\"{{cdn 'https://cdn11.bigcommerce.com/s-z2qdisybty/product_images/uploaded_images/banner-head.png?t=1623904607'}}\" >\n                </div>\n                    <div class=\"heroCarousel-content\">\n                <h2 class=\"heroCarousel-title animation-element invi slide-left-long delay-1000\">Experience the Iconic Treatment\n                    </h2>\n                <p class=\"heroCarousel-description animation-element invi slide-left-long delay-1300\">Shop for affordable, eco-friendly, and \n                    unbelievably comfortable mattresses.\n                    </p>\n                        <span class=\"heroCarousel-action animation-element invi bounce-in duration-1500\">Shop Mattresses</span>\n                </div>\n\n\n            </div>\n        </a>      \n\n        <!-- {{#each carousel.slides}}\n        <a href=\"{{url}}\">\n            <div class=\"heroCarousel-slide {{../theme_settings.hero_align}} {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\n                <div class=\"heroCarousel-image-wrapper\" {{#and image_height image_width}}style=\"height: {{multiply (divide image_height image_width) 90}}vw\"{{/and}}>\n                    <img class=\"heroCarousel-image\" {{#if @first}}src{{else}}data-lazy{{/if}}=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"\n                    {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n                </div>\n                {{#if heading}}\n                    {{> components/f/hero/carousel-content show_background=true theme_settings=../../theme_settings}}\n                {{else}}\n                    {{#if text}}\n                        {{> components/f/hero/carousel-content show_background=true theme_settings=../../../theme_settings}}\n                    {{else}}\n                        {{#if button_text}}\n                            {{> components/f/hero/carousel-content show_background=false theme_settings=../../../../theme_settings}}\n                        {{/if}}\n                    {{/if}}\n                {{/if}}\n            </div>\n        </a>\n        {{/each}} -->\n        <!-- <a href=\"/luxe-gel-memory-foam-mattress/\">\n            <div class=\"heroCarousel-slide\">\n                <div class=\"heroCarousel-image-wrapper\">\n                   <video style=\"width:100%;height:100%\"  autoplay loop>\n                    <source src=\"https://store-z2qdisybty.mybigcommerce.com/product_images/IS%20-%20Homepage%20Banner%202.mp4\" type=\"video/mp4\">\n                    Your browser does not support the video tag.\n                    <video>\n                </div>\n                    <div class=\"heroCarousel-content\">\n   <h2 class=\"heroCarousel-title\">Layered Luxury</h2>\n   <p class=\"heroCarousel-description\">Indulge yourself in lavish sleep with our top-of-the-line spinal support and pressure relief.</p>\n        <span class=\"heroCarousel-action\">Shop Luxe</span>\n</div>\n\n\n            </div>\n        </a>     -->\n\n    </section>\n\n","components/f/hero/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    {{#if heading}}<h{{#if @index '===' 0}}1{{else}}2{{/if}} class=\"heroCarousel-title animation-element invi slide-left-long delay-1000\">{{{heading}}}</h{{#if @index '===' 0}}1{{else}}2{{/if}}>{{/if}}\n    {{#if text}}<p class=\"heroCarousel-description animation-element invi slide-left-long delay-1300\">{{{text}}}</p>{{/if}}\n    {{#if button_text}}\n        <span class=\"animation-element invi bounce-in duration-1500 {{#if show_background}}heroCarousel-action{{else}}button button--primary{{/if}} \">{{button_text}}</span>\n    {{/if}}\n</div>\n"}
