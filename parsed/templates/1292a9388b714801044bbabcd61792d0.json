{"components/f/category/sub-categories":"<div class=\"grid gridColumns--{{theme_settings.subcategories_grid_column_count}} subCategory-grid {{#unless category.total_products}}{{#if (length category.subcategories) '<=' 4}}grid-center{{/if}}{{/unless}}\">\n    {{#each category.subcategories}}\n        <div class=\"gridColumn\">\n            {{#if image}}<div class=\"subCategory-image\"><a href=\"{{url}}\"><img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size'}}\" alt=\"{{name}}\"></a></div>{{/if}}\n            <div class=\"subCategory-name {{#if image}}subCategory-name--center{{/if}}\"><a href=\"{{url}}\">{{name}}</a></div>\n        </div>\n    {{/each}}\n</div>\n"}
